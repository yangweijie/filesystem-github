{
  "name": "pkg6/flysystem-github",
  "description": "Flysystem adapter for the Github storage.",
  "license": "MIT",
  "require": {
    "php": "^7.2.5 || ^8.0",
    "league/flysystem": "^1.0",
    "knplabs/github-api": "^3.0",
    "guzzlehttp/guzzle": "^6|^7",
    "http-interop/http-factory-guzzle": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "pkg6\\flysystem\\github\\": "src/"
    }
  },
  "require-dev": {
    "laravel/framework": "^6|^7|^8",
    "phpunit/phpunit": "^6|^7|^8|^9",
    "mockery/mockery": "^1.2.3"
  },
  "autoload-dev": {
    "psr-4": {
      "pkg6\\flysystem\\github\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
